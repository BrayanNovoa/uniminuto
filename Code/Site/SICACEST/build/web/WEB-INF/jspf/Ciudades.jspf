<%@ page pageEncoding="UTF-8" %>
<%
    try{
        String ciudad;
        consulta="SELECT ciudad FROM tb_ciudades ORDER BY departamento_cod;";
        stpr = con.createStatement();
        rspr = stpr.executeQuery(consulta);
        while(rspr.next()){
            ciudad=rspr.getString(1);
            out.print("<option>"+ciudad+"</option>");
        }
        }catch(Exception e){
        System.out.println("Error: "+e);
    }
%>
